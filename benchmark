#!/bin/bash
#SBATCH -A cc3uv3p # 2017 XSEDE Applications of Parallel Computing Course Allocation
#SBATCH -J benchmark
#SBATCH -o benchmark.txt
#SBATCH -p GPU-shared
#SBATCH --gres=gpu:k80:1
#SBATCH -t 00:10:00
#SBATCH -N 1         

./gpu -n 5000
./gpu -n 10000 
./gpu -n 20000
./gpu -n 40000 
./gpu -n 80000 
./serial -n 5000 
./serial -n 10000 
./serial -n 20000 
./serial -n 40000 
./serial -n 80000  
./autograder -s benchmark.txt -v gpu
